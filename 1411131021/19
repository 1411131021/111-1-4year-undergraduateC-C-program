// Fig. 5.13: fig05_13.c
// Randomizing die-rolling program.

#include <stdlib.h>
#include <stdio.h>
#include <math.h>

int main(void)
{
unsigned int i;
    unsigned int n, m, temp;
    printf("Enter n: ");
    scanf_s("%u", &n);

    for (i = 1; i <= pow(10, n); ++i) {
        temp = pow(10,n);
        m = rand() % temp;
      printf("%lf", (double)m/pow(10,n));
      if (i % 5 == 0)
      {
          puts("");
      }
    }
} 
